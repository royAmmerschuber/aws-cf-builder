{
    "$schema": "./_schema.json",
    "resources": {
        "event_rule":{
            "provides": true,
            "childResources": {
                "cloudwatch":["event_target"]
            }
        },
        "event_target":{
            "nameParts": ["rule","_alias"],
            "inherits": {
                "rule":{"resource":"aws_cloudwatch_event_rule", "attribute":"name"}
            }
        }
    },
    "datasources": {}
}