{
    "$schema": "./_schema.json",
    "resources": {
        "function":{
            "nameParts": ["function_name"],
            "childResources": {
                "lambda":["permission","event_source_mapping","alias"]
            },
            "provides":"lfunc"
        },
        "permission":{
            "nameParts": ["function_name","_alias"],
            "inherits":{
                "function_name":["lfunc","functionName"]
            }
        },
        "event_source_mapping":{
            "nameParts": ["function_name","_alias"],
            "inherits": {
                "function_name":["lfunc","functionName"]
            }
        },
        "alias":{
            "nameParts": ["function_name","name"],
            "inherits": {
                "function_name":["lfunc","functionName"]
            }
        }
    },
    "datasources": {
        "function":{
            "childResources": {
                "lambda":["permission","event_source_mapping","alias"]
            },
            "provides":"lfunc"
        }
    }
}