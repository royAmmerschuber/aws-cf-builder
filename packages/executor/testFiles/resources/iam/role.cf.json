            }
        },
        "Role3Role": {
            "Type": "AWS::IAM::Role",
            "Properties": {
                "RoleName": "peter",
                "AssumeRolePolicyDocument": {
                    "Version": "2012-10-17",
                    "Statement": []
                }
            }
        },
        "Role3InstanceProfile": {
            "Type": "AWS::IAM::InstanceProfile",
            "Properties": {
                "InstanceProfileName": "peter",
                "Roles": [
                    {
                        "Ref": "Role3Role"
                    }
                ]
            }
        },
        "Role4Role": {
            "Type": "AWS::IAM::Role",
            "Properties": {
                "RoleName": {
                    "Fn::GetAtt": [
                        "RoleInstanceProfile",
                        "Arn"
                    ]
                },
                "AssumeRolePolicyDocument": {
                    "Version": "2012-10-17",
                    "Statement": []
                },
                "Path": "peter"
            }
        },
        "Role4InstanceProfile": {
            "Type": "AWS::IAM::InstanceProfile",
            "Properties": {
                "Roles": [
                    {
                        "Ref": "Role4Role"
                    }
                ]
            }
        },
        "Role5Role": {
            "Type": "AWS::IAM::Role",
            "Properties": {
                "RoleName": {
                    "Ref": "RoleInstanceProfile"
                },
                "AssumeRolePolicyDocument": {
                    "Version": "2012-10-17",
                    "Statement": []
                }
            }
        },
        "Role5InstanceProfile": {
            "Type": "AWS::IAM::InstanceProfile",
            "Properties": {
                "InstanceProfileName": "peter",
                "Roles": [
                    {
                        "Ref": "Role5Role"
                    }
                ]
            }
        }
    }
}